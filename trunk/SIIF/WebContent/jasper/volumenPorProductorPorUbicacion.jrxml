	<queryString>
		<![CDATA[select p.*, (select sum(cantidadMts)
           from fiscalizacion
           where localizacion_fk = p.id
           and  periodoForestal = $P{periodo}) as cantidadMts
,
            (select sum(s.cantidadMts)
                      from  guiaforestal g
                      inner join  subimporte s
                      on s.guiaForestal_fk = g.id
                      where g.periodoForestal = $P{periodo}
                      and g.localizacion_fk = p.id
                      group by localizacion_fk ) as cantidadMtsGuia


from
x071vm20_siif.Localizacion p
where p.id = $P{idLocalizacion}
or
p.pmf_fk = $P{idLocalizacion}
or
p.tranzon_fk = $P{idLocalizacion}
or
p.marcacion_fk = $P{idLocalizacion}
or
p.marcacion_fk in (
        select id
        from  x071vm20_siif.Localizacion
        where tranzon_fk = $P{idLocalizacion})
or
p.tranzon_fk in (
    select id
    from  x071vm20_siif.Localizacion m
    where pmf_fk = $P{idLocalizacion}
    )
or
p.marcacion_fk in (
    select id
    from  x071vm20_siif.Localizacion
    where tranzon_fk in (
            select id
            from  x071vm20_siif.Localizacion m
            where  pmf_fk = $P{idLocalizacion} )
    )]]>
	</queryString>
