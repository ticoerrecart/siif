<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="subReporteVolumenGFBPorProductorPorUbicacion" pageWidth="112" pageHeight="15" whenNoDataType="AllSectionsNoDetail" columnWidth="112" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.scriptlethandling" value="0"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="PATH_SUB_REPORTES" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[".\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="periodo" class="java.lang.String"/>
	<parameter name="idProductor" class="java.lang.Long"/>
	<parameter name="idPMF" class="java.lang.Long"/>
	<parameter name="idTranzon" class="java.lang.Long"/>
	<parameter name="idMarcacion" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select 	sum(si.cantidadMts) as cantidadMts

from 	x071vm20_siif.GuiaForestal gf, x071vm20_siif.SubImporte si, x071vm20_siif.Rodal r, 		x071vm20_siif.Marcacion m

where 	gf.periodoForestal = $P{periodo} and gf.entidad_fk = $P{idProductor}
	and si.guiaforestal_fk = gf.id and gf.rodal_fk = r.id
	and r.marcacion_fk = m.id && m.id = $P{idMarcacion}]]>
	</queryString>
	<field name="cantidadMts" class="java.lang.Double"/>
	<variable name="totalGuia" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$F{cantidadMts}]]></variableExpression>
	</variable>
