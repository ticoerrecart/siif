					<paragraph lineSpacing="Single"/>
				</textElement>
				<textFieldExpression><![CDATA[$F{cantidadMts}]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement key="staticText-20" mode="Transparent" x="0" y="0" width="112" height="15" forecolor="#000000" backcolor="#FFFFFF">
					<printWhenExpression><![CDATA[$F{cantidadMts} == null]]></printWhenExpression>
				</reportElement>
